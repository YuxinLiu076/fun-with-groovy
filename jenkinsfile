node {
	stage('SCM') {
		echo 'Gathering code from version control'
	}
    stage('Build') {
        echo 'Building....'
        sh 'dotnet --version'
        sh 'dotnet build ConsoleApp'
    }
    stage('Test') {
        echo 'Testing....'
    }
    stage('Deploy') {
        echo 'Deploying....'
    }
}